  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.0.0",
    "@iconify-json/carbon": "^1.2.11",
    "@types/node": "^24.1.0",
    "@unocss/eslint-config": "^66.3.3",
    "@unocss/eslint-plugin": "^66.3.3",
    "@unocss/reset": "^66.3.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue-macros/volar": "^0.30.15",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.32.0",
