#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/Workspace/Applications/talex-touch/node_modules/.pnpm/vite@4.4.9_@types+node@20.4.7_sass@1.66.1/node_modules/vite/bin/node_modules:/mnt/e/Workspace/Applications/talex-touch/node_modules/.pnpm/vite@4.4.9_@types+node@20.4.7_sass@1.66.1/node_modules/vite/node_modules:/mnt/e/Workspace/Applications/talex-touch/node_modules/.pnpm/vite@4.4.9_@types+node@20.4.7_sass@1.66.1/node_modules:/mnt/e/Workspace/Applications/talex-touch/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/Workspace/Applications/talex-touch/node_modules/.pnpm/vite@4.4.9_@types+node@20.4.7_sass@1.66.1/node_modules/vite/bin/node_modules:/mnt/e/Workspace/Applications/talex-touch/node_modules/.pnpm/vite@4.4.9_@types+node@20.4.7_sass@1.66.1/node_modules/vite/node_modules:/mnt/e/Workspace/Applications/talex-touch/node_modules/.pnpm/vite@4.4.9_@types+node@20.4.7_sass@1.66.1/node_modules:/mnt/e/Workspace/Applications/talex-touch/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
