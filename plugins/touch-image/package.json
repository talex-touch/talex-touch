{
  "name": "touch-image",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "talex-touch": {
    "icon": {
      "type": "class",
      "value": "i-ri-image-2-line"
    },
    "plugin": {
      "dev": {
        "enable": true,
        "address": "http://127.0.0.1:6001"
      }
    },
    "build": {
      "files": [
        "README.md"
      ],
      "secret": {
        "pos": "Tuff",
        "addon": [
          "windows",
          "darwin",
          "linux"
        ]
      },
      "verify": {
        "enable": true,
        "online": "always"
      },
      "version": {
        "update": "auto",
        "downgrade": false
      }
    }
  },
  "dependencies": {
    "@iconify-json/ri": "^1.2.5",
    "@talex-touch/unplugin-export-plugin": "^0.2.13",
    "@talex-touch/utils": "workspace:^",
    "@unocss/preset-icons": "^66.3.3",
    "@unocss/preset-uno": "^66.3.3",
    "unocss": "^66.3.3",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vue-tsc": "^3.0.4"
  }
}
