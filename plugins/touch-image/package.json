{
  "name": "touch-image",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview"
  },
  "talex-touch": {
    "icon": {
      "type": "class",
      "value": "i-ri-image-2-line"
    },
    "plugin": {
      "dev": {
        "enable": true,
        "address": "http://127.0.0.1:6001"
      }
    },
    "build": {
      "files": [
        "README.md"
      ],
      "secret": {
        "pos": "TalexTouch",
        "addon": [
          "windows",
          "darwin",
          "linux"
        ]
      },
      "verify": {
        "enable": true,
        "online": "always"
      },
      "version": {
        "update": "auto",
        "downgrade": false
      }
    }
  },
  "dependencies": {
    "@iconify-json/ri": "^1.1.12",
    "@talex-touch/unplugin-export-plugin": "^0.2.13",
    "@talex-touch/utils": "workspace:^",
    "@unocss/preset-icons": "^0.55.4",
    "@unocss/preset-uno": "^0.55.4",
    "unocss": "^0.55.4",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.3.4",
    "sass": "^1.66.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.15"
  }
}
