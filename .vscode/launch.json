  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Main Process",
      "skipFiles": [
        "<node_internals>/**"
      ],
      // "program": "${file}",
      "autoAttachChildProcesses": true,
      "cwd": "${workspaceFolder}/app/main",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": [
        "-F",
        "main",
        "dev",
      ],
      "env": {
        "REMOTE_DEBUGGING_PORT": "5173"
      }
    }
  ],
  "compounds": [
    {
      "name": "Launch Program",
      "configurations": [
        "Debug Main Process",
      ],
      "presentation": {
        "order": 1
      }
    }
  ]
}